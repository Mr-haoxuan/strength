{
  "extends": "./tsconfig/tsconfig.base",
  "include": [
    "src/**/*",
    "typings",
    "node_modules/@msfe/react-mobile-scripts/public-config/types/**/*.d.ts"
  ],
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "module": "esnext",
    "baseUrl": "src",
    "lib": ["dom", "dom.iterable", "esnext"]
  },
  "exclude": ["node_modules", "src/autod/apis/*"],
  "typeRoots": ["node_modules/@types", "./typings"]
}
